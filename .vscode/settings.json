{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "vetur.validation.template": false,
  "cSpell.words": [
    "devtool",
    "docsearch",
    "frontmatter",
    "globby",
    "nprogress",
    "prefetch",
    "preload",
    "slugify",
    "unmount",
    "vuepress"
  ]
}
